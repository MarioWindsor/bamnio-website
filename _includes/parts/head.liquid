<!doctype html>
<html lang="en">
<head>

	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no, viewport-fit=cover" />


	<title>{{ title | append: ' | ' }}{{ site.name }}</title>
	<base href="/">

	
	<!-- Metadata -->
	<!-- Short description of the document (limit to 150 characters) -->
	<meta name="description" content="{{ description | default: site.description }}">


	<!-- Authors -->
	<!-- Links to information about the author(s) of the document -->
	<meta name="author" content="{{ site.author }}">
	<link rel="author" href="{{ site.humans }}">


	<!-- SEO -->
	<!-- Control the behavior of search engine crawling and indexing -->
	<meta name="robots" content="index,follow"><!-- All Search Engines -->


	<!-- Favicons -->
	<link rel="apple-touch-icon" sizes="180x180" href="media/favicon/apple-icon-180x180.png">
	<link rel="icon" type="image/png" sizes="32x32" href="media/favicon/favicon-32x32.png">
	<link rel="icon" type="image/png" sizes="16x16" href="media/favicon/favicon-16x16.png">
	<link rel="manifest" href="media/favicon/manifest.json">


	<!-- ~ iOS ~ -->
	<!-- Disable automatic detection and formatting of possible phone numbers -->
	<meta name="format-detection" content="telephone=no">


	<!-- Open Graph -->
	<meta property="og:type" content="website">
	<meta property="og:title" content="{{ title | append: ' | ' }}{{ site.name }}">
	<meta property="og:description" content="{{ description | default: site.description }}">
	<meta property="og:url" content="{{ site.baseUrl }}">
	<meta property="og:image" content="{{ site.image }}">
	<meta property="og:site_name" content="{{ site.name }}">


	<!-- Schema.org / Google+ -->
	<meta itemprop="name" content="{{ title | append: ' | ' }}{{ site.name }}">
	<meta itemprop="description" content="{{ description | default: site.description }}">
	<meta itemprop="image" content="{{ site.image }}">


	<!-- Mako CSS -->
	<link rel="stylesheet" type="text/css" href="css/0-root.css">
	<link rel="stylesheet" type="text/css" href="css/1-normalise.css">
	<link rel="stylesheet" type="text/css" href="css/2-base.css">
	<link rel="stylesheet" type="text/css" href="css/3-layout.css">
	<link rel="stylesheet" type="text/css" href="css/4-function.css">
	<link rel="stylesheet" type="text/css" href="css/5-theme.css">

	<!-- Components -->
	<link rel="stylesheet" type="text/css" href="css/components/card.css">
	<link rel="stylesheet" type="text/css" href="css/components/carousel.css">
	<link rel="stylesheet" type="text/css" href="css/components/video-embed.css">

	<!-- Parts -->
	<link rel="stylesheet" type="text/css" href="css/parts/header.css">
	<link rel="stylesheet" type="text/css" href="css/parts/footer.css">

	<!-- Layouts -->
	<link rel="stylesheet" type="text/css" href="css/layouts/home.css">

	<!-- Font -->
	<link rel="preconnect" href="https://fonts.googleapis.com">
	<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
	<link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">


	<!-- Box Icon -->
	<link href='https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css' rel='stylesheet'>
	<script src="https://unpkg.com/boxicons@2.1.4/dist/boxicons.js"></script>


	<!-- Typing JS -->
	<!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/typed.js/2.0.11/typed.min.js" integrity="sha512-BdHyGtczsUoFcEma+MfXc71KJLv/cd+sUsUaYYf2mXpfG/PtBjNXsPo78+rxWjscxUYN2Qr2+DbeGGiJx81ifg==" crossorigin="anonymous"></script> -->


	<!-- Netlify Identity -->
	<script type="text/javascript" src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>


	<!-- Lottie Player -->
	<!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/lottie-web/5.12.2/lottie.min.js"></script> -->



	<!-- Google Tag Manager -->
	<!-- !!! ENTER TAG MANAGER SCRIPT HERE !!! -->
	<!-- END: Google Tage Manager -->


</head>
<body class="body" id="body">


